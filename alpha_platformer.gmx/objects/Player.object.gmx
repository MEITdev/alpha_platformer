        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Collision with an enemy
var above_enemy = y &lt; other.y + vspd;
var falling = vspd &gt; 0;
if (above_enemy &amp;&amp; (falling || state = player_ledge_grab_state))
{
    // make sure we do not kill 2 enemies at once
    if (!place_meeting(x, yprevious, Solid))
    {
        y = yprevious;
    }

    // now move the enemy right on top of the enemy
    while(!place_meeting(x, y + 1, other))
    {
        y++;
    }
    
    // Kill the enemy
    with(other)
    {
        instance_destroy();
    }
    
    // Bounce off the enemy
    vspd = jspd / 1.5;
    // Play the sound
    audio_play_sound(snd_enemy_hurt, 5, false);
}
else
{
    // Take damage
    if (state != player_hurt_state)
    {
        image_blend = make_colour_rgb(220,150,150);
        vspd = jspd / 2;
        hspd = sign(x - other.x) * 6;
        
        state = player_hurt_state;
        
        Move(Solid);
        
        if (instance_exists(PlayerStats))
        {
            PlayerStats.player_hp -= 1;
        }
    }
}
</string>
